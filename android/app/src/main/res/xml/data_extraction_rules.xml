<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Include all shared preferences by default -->
        <include domain="sharedpref" path="." />
    </cloud-backup>
    <device-transfer>
        <!-- Allow device transfer for user data -->
        <include domain="file" path="." />
        <include domain="sharedpref" path="." />
    </device-transfer>
</data-extraction-rules>